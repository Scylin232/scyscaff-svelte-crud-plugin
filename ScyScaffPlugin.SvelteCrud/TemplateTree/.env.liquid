{{~ if config.auth == "auth0" ~}}
AUTH0_ID="GET_FROM_AUTH0"
AUTH0_SECRET="GET_FROM_AUTH0"
AUTH0_ISSUER="GET_FROM_AUTH0"
AUTH0_AUDIENCE="https://auth/api"
{{~ end ~}}

{{~ for compose_service in compose_services ~}}
  {{~ if compose_service.type == "Framework" ~}}
    {{~ for port in compose_service.ports ~}}
{{ compose_service.container_name | string.replace "-" "_" | string.upcase }}_PORT={{ port.key }}
    {{~ end ~}}
  {{~ end ~}}
{{~ end ~}}

JWT_SECRET="RANDOM_STRING_THAT_YOU_HAVE_TO_GENERATE"
REDIRECT_URL="http://localhost:5173"